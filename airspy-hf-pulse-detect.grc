      blk = gr_detect_pulses.blk'
    affinity: ''
    alias: ''
    comment: ''
    debounce_samples: '10'
    filename: '"output.csv"'
    maxoutbuf: '0'
    min_snr_db: '6'
    minoutbuf: '0'
    output_type: '''file'''
    port: '0'
    pulse_len_ms: pulse_len_ms
    samp_rate: samp_rate / decimation_factor
    verbose: 'True'
  states:
    _io_cache: ('Pulse detector', 'blk', [('output_type', "'file'"), ('verbose', 'True'),
